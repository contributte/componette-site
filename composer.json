{
  "name": "contributte/componette-site",
  "description": "Addons, plugins and components for Nette Framework",
  "keywords": [
    "nette",
    "components",
    "addons",
    "extensions"
  ],
  "homepage": "https://componette.org",
  "type": "project",
  "license": [
    "MIT"
  ],
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    },
    {
      "name": "Componette Contributors",
      "homepage": "https://github.com/contributte/contributte-site/graphs/contributors"
    }
  ],
  "require": {
    "php": ">=8.5",
    "ext-curl": "*",
    "ext-mbstring": "*",
    "ext-mysqli": "*",
    "ext-pdo": "*",
    "ext-simplexml": "*",
    "contributte/application": "^0.6.1",
    "contributte/bootstrap": "^0.6.0",
    "contributte/cache": "^0.6.0",
    "contributte/console": "^0.10.1",
    "contributte/di": "dev-master",
    "contributte/forms": "^0.5.2",
    "contributte/http": "^0.4.1",
    "contributte/latte": "^0.6.1",
    "contributte/security": "^0.4.0",
    "contributte/tracy": "^0.6.0",
    "contributte/utils": "^0.7.0",
    "contributte/webpack": "^2.3",
    "nette/caching": "^3.4",
    "nette/robot-loader": "^4.0",
    "nextras/dbal": "^5.0",
    "nextras/migrations": "^3.4",
    "nextras/orm": "dev-main"
  },
  "require-dev": {
    "contributte/qa": "^0.4",
    "contributte/tester": "^0.4",
    "nextras/orm-phpstan": "^2.0",
    "phpstan/phpstan": "^2.0",
    "phpstan/phpstan-nette": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "App\\Model\\": "app/model",
      "App\\Modules\\": "app/modules"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/cases"
    ]
  },
  "config": {
    "platform": {
      "php": "8.5"
    },
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
